{
  "name": "dnd-lite-monorepo",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "install:all": "npm install --workspaces --include-workspace-root",
    "dev": "concurrently \"uvicorn server.src.main:app --reload\" \"npm run dev --prefix client\""
  },
  "devDependencies": {
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "concurrently": "^9.1.2"
  }
}
